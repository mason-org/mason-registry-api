{
  "functions": {
    "api/repo/**/*.rs": {
      "runtime": "vercel-rust@3.1.2"
    },
    "api/npm/**/*.rs": {
      "runtime": "vercel-rust@3.1.2"
    }
  },
  "rewrites": [
    {
      "source": "/api/npm/:path((?!@).*)",
      "destination": "/api/npm/_/:path"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/williamboman/mason-registry-api",
      "permanent": false
    }
  ]
}
